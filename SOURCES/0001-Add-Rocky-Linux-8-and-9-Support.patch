diff --git a/internal/distro/rhel8/distro.go b/internal/distro/rhel8/distro.go
index a762ccd..d5c280d 100644
--- a/internal/distro/rhel8/distro.go
+++ b/internal/distro/rhel8/distro.go
@@ -155,6 +155,26 @@ func NewCentos() distro.Distro {
 	return newDistro("centos", 0)
 }
 
+func NewRocky84() distro.Distro {
+	return newDistro("rocky", 4)
+}
+
+func NewRocky85() distro.Distro {
+	return newDistro("rocky", 5)
+}
+
+func NewRocky86() distro.Distro {
+	return newDistro("rocky", 6)
+}
+
+func NewRocky87() distro.Distro {
+	return newDistro("rocky", 7)
+}
+
+func NewRocky88() distro.Distro {
+	return newDistro("rocky", 8)
+}
+
 func newDistro(name string, minor int) *distribution {
 	var rd distribution
 	switch name {
@@ -184,6 +204,19 @@ func newDistro(name string, minor int) *distribution {
 			runner:             &runner.CentOS{Version: uint64(8)},
 			defaultImageConfig: defaultDistroImageConfig,
 		}
+	case "rocky":
+		rd = distribution{
+			name:               fmt.Sprintf("rocky-8%d", minor),
+			product:            "Rocky Linux",
+			osVersion:          fmt.Sprintf("8.%d", minor),
+			releaseVersion:     "8",
+			modulePlatformID:   "platform:el8",
+			vendor:             "rocky",
+			ostreeRefTmpl:      "rocky/8/%s/edge",
+			isolabelTmpl:       fmt.Sprintf("Rocky-8-%d-dvd-%%s", minor),
+			runner:             &runner.RHEL{Major: uint64(8), Minor: uint64(minor)},
+			defaultImageConfig: defaultDistroImageConfig,
+		}
 	default:
 		panic(fmt.Sprintf("unknown distro name: %s", name))
 	}
